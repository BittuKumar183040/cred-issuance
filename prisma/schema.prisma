generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model assignment {
  id            String   @id
  username      String   @unique
  issued_by     String
  issued_at     BigInt
  updated_at    BigInt
  issued_status String
}
